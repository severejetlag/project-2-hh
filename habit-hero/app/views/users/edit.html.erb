<%= form_with model: @user do |form| %>
  <label>first:</label>
  <%= form.text_field :first_name%>
  <label>last</label>
  <%= form.text_field :last_name%>
  <label>email</label>
  <%= form.text_field :email%>
  <label>password:</label>
  <%= form.password_field :password%>
  <%= form.submit "Submit" %>
<% end%>
