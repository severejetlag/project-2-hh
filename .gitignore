# Ignore bundler config.
/habit-hero/.bundle

# Ignore all logfiles and tempfiles.
/habit-hero/log/*
/habit-hero/tmp/*
!/habit-hero/log/.keep
!/habit-hero/tmp/.keep

/habit-hero/node_modules
/habit-hero/yarn-error.log

/habit-hero/.byebug_history
